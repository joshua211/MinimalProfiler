<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;net5.0</TargetFrameworks>
    <EnableUnsafeBinaryFormatterSerialization>true</EnableUnsafeBinaryFormatterSerialization>
    <PackageId>minimalprofiler</PackageId>
    <Version>1.0.0</Version>
    <Titel>Minimal Net Core Profiler </Titel>
    <Authors>Joshua Hillmann</Authors>
    <PackageTags>dotnetCore;Profiler;MinimalProfiler;Profiling;TimeMeasurement;ExecutionTime</PackageTags>
    <PackageDescription>
      A minimal, code only profiler written in C# for net core. Can track method execution time 
      by adding a single attribute to a method and log results.
    </PackageDescription>
    <PackageProjectUrl>
      https://github.com/joshua211/MinimalProfiler/tree/master
    </PackageProjectUrl>
    <RepositoryUrl>
      https://github.com/joshua211/MinimalProfiler/tree/master
    </RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Lib.Harmony" Version="2.0.4" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="5.0.0" />
  </ItemGroup>


</Project>
